@prefix sh:  <http://www.w3.org/ns/shacl#> .
@prefix ex:  <http://example.org/schema#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .

ex:MechanismShape a sh:NodeShape ;
  sh:targetClass ex:Mechanism ;
  sh:property [ sh:path rdfs:label ; sh:minCount 1 ] ;
  sh:property [ sh:path ex:hasExternalRef ; sh:minCount 1 ] ;
  sh:property [ sh:path ex:hasGateCore ; sh:minCount 1 ] .

ex:GateCoreEntryShape a sh:NodeShape ;
  sh:targetClass ex:GateCoreEntry ;
  sh:property [ sh:path ex:geneSymbol ; sh:minCount 1 ] .

